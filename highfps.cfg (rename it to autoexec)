// IMPORTANT! - Remove "//" infront of the commands to take effect

// KEY-LIST------------------------------------------------------

// "F7"	    - Reexecute this Autoexec
// "o"      - Reboots the audio engine

// GAMEPLAY-SETTINGS---------------------------------------------

m_rawinput "1"                                                                  // Takes the direct input of your mouse
m_filter "0"                                                                    // Makes sure any kind of mouse filtering is off
m_acceleration "0"                                                              // Once again this makes sure mouse acceleration is off
m_customaccel "0"                                                               // Remove any kind of mouse acceleration by RawAccel 
m_customaccel_exponent "0"                                                      // Remove any kind of mouse acceleration by RawAccel  
m_mouseaccel1 "0"                                                               // Remove any kind of mouse acceleration by RawAccel  
m_mouseaccel2 "0"                                                               // Remove any kind of mouse acceleration by RawAccel  
m_customaccel_max "0"                                                           // Remove any kind of mouse acceleration by RawAccel  
player_setting_damage_closes_deathbox_menu "0"                                  // Your inventory wont auto-close anymore when receiving damage
hud_setting_minimapRotate "1"                                                   // This setting allows the minimap to rotate with you (default 0)
sprint_view_shake_style "1"                                                     // Less headbob
mat_vsync "0"                                                                   // Ensure VSync is disabled by default
r_fullscreen 1                                                                  // Fullscreen mode (adding this command before some pro idiots are playing in borderless fullscreen)
hud_setting_pingDoubleTapEnemy "1"                                              // Double tap to ping enemy (default 0)
fov_disableAbilityScaling "1"                                                   // Really good for octane and bloodhound mains (default 0)
developer "1"                                                                   // Now you can see time in your lobby

// CUSTOM-BINDS--------------------------------------------------

bind "F7" "exec autoexec.cfg"                                                   // If you have accidentally changes something in the video, options just press F7 and the settings will load again
bind "o" "miles_reboot"	                                                        // Reboots the audio engine

// AUDIO-SETTINGS-------------------------------------------------

miles_channels 2                                                                // Virtual surround sound (Number of audio channels 2 (stereo), 6 (5.1), 8 (7.1). (0 is default))
miles_occlusion 0                                                               // Enable / Disable virtual sound (default 1)(this command got removed on season 10)
miles_occlusion_force 0                                                         // Force virtual sound on / off (default -1)
miles_occlusion_partial 0                                                       // Enable / Disable virtual sound, partial (default 1)
sound_without_focus 1                                                           // Game does not stop to play sound when tabbed out
snd_mixahead "0.05"                                                             // Reduce the audio delay  
snd_headphone_pan_exponent "2"                                                  // Makes sounds you are facing towards louder, distant sounds in the middle much better. 
                                                                                // But it also makes everything sound much closer than it is when you're facing it.
snd_musicvolume "0"                                                             // Disables unnecessary sounds
snd_setmixer PlayerFootsteps vol 0.1                                            // Reduce your footsteps 
snd_setmixer GlobalFootsteps vol 1.2                                            // Increase other footsteps 
snd_async_fullyasync "1"                                                        // Sound doesn't play until data arrives
cl_footstep_event_max_dist 4000                                                 // Increase the enemy footsteps range (default 2500)
miles_occlusion_server_sounds_per_frame 200
miles_max_sounds_per_server_frame "400"                                             

// GRAPHICS-BASED-COMMANDS---------------------------------------

fps_max 0                                                                       // Unlocks framerate
mat_compressedtextures 1                                                        // The game use compressed textures for optimal performance 
cl_ejectbrass 0                                                                 // Disable empty bullets
r_fastzreject -1                                                                // Renders first pass of scene with Z-buffer on the CPU and 2nd on the GPU (-1 = lets hardware decide; 1 = NVIDIA)
r_norefresh 1                                                                   // Do not store useless and unused frame time variable (default 0)
player_disallow_negative_frametime "0"                                          // Ignore negative frame times for client commands
cl_drawmonitors 0                                                               // Enable / Disable animations and images on the banners of the detachment (default 1)
stream_cache_high_priority_static_models 1                                      // Don't preload static models (default 1)
stream_cache_preload_from_rpak 1                                                // Do not preload to cache from rpak (0 = Off; 1 = Preload stat models; 2 = Always preload) (default 2)
staticProp_max_scaled_dist 1500                                                 // The distance at which static objects will be rendered on the map
stream_drop_unused 1                                                            // Unload unused textures more aggressively (frees up GPU memory) (default 0)
mp_usehwmmodels "-1"                                                            // Do not use or load high quality characters
mp_usehwmvcds "-1"                                                              // Do not use or load high quality character facial expressions

// MODEL-BASED-COMMANDS------------------------------------------

violence_ablood 0                                                               // Remove blood effect
violence_agibs 0                                                                // Remove blood effect
violence_hblood 0                                                               // Remove blood effect for aliens 
violence_hgibs 0                                                                // Remove blood effect for aliens
g_ragdoll_fadespeed "10000"                                                     // The rate of ragdoll fading
g_ragdoll_lvfadespeed "10000"                                                   // The rate of ragdoll fading in low violence
cl_bones_incremental_blend "1"                                                  // Don't re-render bones; remove unnecessary load (default 0)
cl_SetupAllBones "0"                                                            // Do not force every animation component of a model to be set up

// OVERRIDE-COMMANDS---------------------------------------------

mat_light_edit "1"                                                              // Changes to the map lighting (1-On / 0-Off) (default 0) (mat_sun_highlight_size is needed)
map_settings_override 1                                                         // On allows you to adjust the value of cvars (fog_enable ; mat_bloomscale) (default 0)

// SECRET-COMMANDS-----------------------------------------------

cl_show_splashes 0                                                              // Remove water Splashes
dof_enable 0                                                                    // Disable depth of field (blurring at distance)
hud_setting_adsDof 0                                                            // Disable depth of field (blurring at distance) when aiming down sights
hud_setting_healthUseOnHold 1                                                   // Use Health item on hold
hud_setting_healthWheelUseOnRelease 1                                           // Use Health item on release
mat_queue_mode "-1"                                                             // A value of -1 uses the default for your system, a value of 0 uses synchronous single thread, 1 uses queued single threaded mode and value of 2 uses multi-threaded mode
mat_diffuse 0                                                                   // Blur (default 1)
mat_disable_bloom 1                                                             // Disables the propagation of the glow from light sources (default 0)
ordnanceSwapSelectCooldown 0.1                                                  // Reduce grenade swap time (inventory) (default 0.25)
sidearmSwapSelectCooldown 0.1                                                   // Reduce weapon swap time (inventory) (default 0.25)
sidearmSwapSelectDoubleTapTime 0.1                                              // Reduce the time to change weapons by double clicking (inventory) (default 0.25)
mat_sun_highlight_size "0"                                                      // Changes the size of the highlight from the sun (default 1)
fog_enable 0                                                                    // Disable fog (not disabled everywhere) (default 1)
fog_enableskybox 0                                                              // Disable fog over the sky (1-On / 0-Off) (default 1)
r_forcecheapwater 1                                                             // Decrease in water quality (default 0)
ssao_blur "0"                                                                   // Blur, reflections and highlights (default 1)
sssss_enable 0                                                                  // Disables Screen Space Subsurface Scattering
r_particle_timescale 3                                                          // Decelerate / Accelerate Particles; Reduces weapon muzzle flare (mendokusaii style) (default 1)
nx_static_lobby_mode 2                                                          // Nintendo switch cvar that got somehow into the main game, "2" reduces loading times
mat_fullbright "1"                                                              // Brighter materials/textures
noise_filter_scale 0                                                            // Removes Film-grain
r_particle_lighting_enable "0"                                                  // Particle lighting (default 1)
lightmap_realtimelight "0"                                                      // If "1" use lightmap to select realtime lighting rendering (default 1)
lightmap_realtimeshadows "0"                                                    // If "1" use shadowmap to select realtime shadow rendering (default 1)
particle_dlights_enable "0"                                                     // System of light particles (default 1)
func_break_max_pieces 0                                                         // Removes various small debris on maps (default 15)
cl_detailfade "0"                                                               // Distance across which detail props fade in
cl_detaildist "0"                                                               // Distance at which detail props are no longer visible
r_ropetranslucent "0"                                                           // Skip simulating ropes

// SHADOWS-COMMANDS----------------------------------------------

r_shadows 0                                                                     // Disables shadows 
shadow_capable "0"                                                              // Shadows (default 1)
shadow_enable "0"                                                               // Shadows Off / On (default 1)
static_shadow_res 0                                                             // Setting the rendering resolution of static shadows (default 4096)
viewmodel_selfshadow "0"                                                        // Disables the shadow of the player model and his weapon (default 1)
csm_world_shadows 0                                                             // Various shadows on the map (default 1)
csm_rope_shadows                                                                // Various wire shadows (default 1)
tweak_light_shadows_every_frame "0"                                             // Shadows
csm_force_no_csm_in_reflections 1
csm_auto_entity 0 

// CONNECTION-AND-CLIENT-SETTINGS--------------------------------
// IMPORTANT! - This Works Great For Me--------------------------

rate "128000"                                                                   // Maximum number of transmitted bytes per second from servers (default 128000)                                                             
cl_cmdrate "120"                                                                // Number of updates to information about yourself on the server per second (default 60)
cl_updaterate_mp "60"                                                           // The number of requests to update the game world from the server per second (default 20)
match_updateRate "60"                                                           // The number of requests to update the game world from the server per second (default 20)
net_compresspackets "0"                                                         // Compress the packets sent to the server
cl_smooth "0"                                                                   // Disable show predictive error smoothing 
cl_smoothtime "0.01"                                                            // Time (seconds) client smoothing from prediction errors (min. 0.01) (max. 2)
cl_pred_optimize "1"                                                            // Predictive optimization (client)
cl_lagcompensation "1"                                                          // Performs server side lag compensation
net_compresspackets_minsize "128"                                               // Compress packages not lower than the set value
net_maxcleartime "0.020346"                                                     // Maximum time in seconds that the client waits before sending the next packet to the server (default 4.0)
host_limitlocal "0"                                                             // Apply cl_cmdrate and cl_updaterate to close the connection
cl_matchmaking_timeout 10                                                       // Raise base matchmaking timeout to decrease potential timeouts (default 0.5)
cl_ranked_reconnect_timeout 300                                                 // Sync to reconnect timeout to allow max reconnectable time (default 15)
cl_resend_timeout 300                                                           // Sync to reconnect timeout to allow max reconnectable time (default 10)
cl_phys_props_enable "0"                                                        // Disable client side physics props
cl_predictweapons "1"                                                           // Perform client side prediction of weapon effects
cl_interp "0"                                                                 
cl_interp_ratio "1"                                                           
cl_updatevisibility "1"                                                       
cl_timeout "30"                                                                                                           
cl_wpn_sway_interp "0" 

// PRIVACY-SETTINGS----------------------------------------------

